    <hal format="hidl" optional="true">
        <name>vendor.lineage.fastcharge</name>
        <version>1.0</version>
        <interface>
            <name>IFastCharge</name>
            <instance>default</instance>
        </interface>
    </hal>
</compatibility-matrix>